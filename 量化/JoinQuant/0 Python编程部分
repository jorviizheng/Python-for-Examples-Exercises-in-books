abs(-100)

def my_abs(x):
	if x>=0:
		return x
	else:
		return -x
my_abs(100)

def power(x):
	return x*x
power(5)

def power1(x,n=2):
	s=1
	while n > 0:
		n = n-1
		s = s*x
	return s
power1(5)


def calc1(numbers):
	sum = 0
	for n in numbers:
		sum = sum + n * n
	return sum
calc1([1,2,3])

